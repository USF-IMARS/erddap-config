    <dataVariable>
        <sourceName>Rrs_671_median</sourceName>
        <destinationName>Rrs_671_median</destinationName>
        <dataType>double</dataType>
        <addAttributes>
            <att name="colorBarMaximum" type="double">0.02</att>
            <att name="colorBarMinimum" type="double">0.0001</att>
            <att name="ioos_category">Optical Properties</att>
            <att name="long_name">Rrs_671 Median</att>
            <att name="Units">null</att>
            <att name="units">sr^-1</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
