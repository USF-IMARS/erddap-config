<dataset type="EDDGridFromNcFiles" datasetID="IMERG_monthly_global_precip" active="true">
    <reloadEveryNMinutes>10080</reloadEveryNMinutes>
    <updateEveryNMillis>10000</updateEveryNMillis>
    <fileDir>/mnt/nvme0n1/IMERG_monthly_global_precip/</fileDir>
    <fileNameRegex>.*\.nc</fileNameRegex>
    <recursive>true</recursive>
    <pathRegex>.*</pathRegex>
    <metadataFrom>last</metadataFrom>
    <matchAxisNDigits>20</matchAxisNDigits>
    <fileTableInMemory>false</fileTableInMemory>
    <addAttributes>
        <att name="cdm_data_type">Grid</att>
        <att name="Contact">null</att>
        <att name="contact">Dan Otis - dotis@usf.edu</att>
        <att name="Conventions">COARDS, CF-1.6, ACDD-1.3</att>
        <att name="creator_email">dotis@usf.edu</att>
        <att name="creator_name">DOTIS</att>
        <att name="creator_type">institution</att>
        <att name="infoUrl">imars.usf.edu</att>
        <att name="institution">NASA Global Precipitation Measurement Mission</att>
        <att name="keywords">precipitation, NASA, IMERG, GPM</att>
        <att name="keywords_vocabulary">GCMD Science Keywords</att>
        <att name="Lat-Lon_Limits">null</att>
        <att name="Lat_Lon_Limits">-90N to 90N -180E to 180E</att>
        <att name="license">[standard]</att>
        <att name="standard_name_vocabulary">CF Standard Name Table v70</att>
        <att name="summary">NASA GPM IMERG Global Monthly Precipitation</att>
        <att name="title">Global Monthly Precipitation (NASA GPM IMERG)</att>
    </addAttributes>
    <axisVariable>
        <sourceName>***fileName,timeFormat=yyyyMMdd,IMERG_(\d{8})_MO_GLOB_precip_ED\.nc,1</sourceName>
        <destinationName>time</destinationName>
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="units">seconds since 1970-01-01T00:00:00Z</att>
        </addAttributes>
    </axisVariable>
    <axisVariable>
        <sourceName>latitude</sourceName>
        <destinationName>latitude</destinationName>
    </axisVariable>
    <axisVariable>
        <sourceName>longitude</sourceName>
        <destinationName>longitude</destinationName>
    </axisVariable>
    <dataVariable>
        <sourceName>precipitation</sourceName>
        <destinationName>precipitation</destinationName>
        <dataType>double</dataType>
        <addAttributes>
            <att name="colorBarMaximum" type="double">500.0</att>
            <att name="colorBarMinimum" type="double">0.0</att>
            <att name="colorBarScale">Linear</att>
            <att name="colorBar">BlueWhiteRed</att>
            <att name="ioos_category">Meteorology</att>
            <att name="long_name">precipitation</att>
            <att name="standard_name">Monthly Accumulated Precipitation</att>
            <att name="Units">null</att>
            <att name="units">mm</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>precipitationQualityIndex</sourceName>
        <destinationName>precipitationQualityIndex</destinationName>
        <dataType>double</dataType>
        <addAttributes>
            <att name="colorBarMaximum" type="double">200.0</att>
            <att name="colorBarMinimum" type="double">0.0</att>
            <att name="colorBarScale">Linear</att>
            <att name="colorBar">BlueWhiteRed</att>
            <att name="ioos_category">Meteorology</att>
            <att name="long_name">Precipitation Quality Index</att>
            <att name="Units">null</att>
            <att name="units">none</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>randomError</sourceName>
        <destinationName>randomError</destinationName>
        <dataType>double</dataType>
        <addAttributes>
            <att name="colorBarMaximum" type="double">100.0</att>
            <att name="colorBarMinimum" type="double">0.0</att>
            <att name="colorBarScale">Linear</att>
            <att name="colorBar">BlueWhiteRed</att>
            <att name="ioos_category">Meteorology</att>
            <att name="long_name">Monthly Accumulated Precipitation Random Error</att>
            <att name="Units">null</att>
            <att name="units">mm</att>
        </addAttributes>
    </dataVariable>
 </dataset>
