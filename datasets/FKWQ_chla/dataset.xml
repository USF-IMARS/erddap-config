<!-- NOTE! Since the source files don't have any metadata, you must add metadata
  below, notably 'units' for each of the dataVariables. -->
<dataset type="EDDTableFromAsciiFiles" datasetID="FKWQ_chl" active="true">
    <reloadEveryNMinutes>10080</reloadEveryNMinutes>
    <updateEveryNMillis>10000</updateEveryNMillis>
    <fileDir>/mnt/sdb/FKWQ_chl/</fileDir>
    <fileNameRegex>Merged_chla.csv</fileNameRegex>
    <recursive>true</recursive>
    <pathRegex>.*</pathRegex>
    <metadataFrom>last</metadataFrom>
    <standardizeWhat>0</standardizeWhat>
    <charset>ISO-8859-1</charset>
    <columnSeparator>,</columnSeparator>
    <columnNamesRow>1</columnNamesRow>
    <firstDataRow>2</firstDataRow>
    <sortedColumnSourceName></sortedColumnSourceName>
    <sortFilesBySourceNames></sortFilesBySourceNames>
    <fileTableInMemory>false</fileTableInMemory>
    <!-- sourceAttributes>
    </sourceAttributes -->
    <!-- Please specify the actual cdm_data_type (TimeSeries?) and related info below, for example...
        <att name="cdm_timeseries_variables">station_id, longitude, latitude</att>
        <att name="subsetVariables">station_id, longitude, latitude</att>
    -->
    <addAttributes>
        <att name="cdm_data_type">TimeSeries</att>
        <att name="Conventions">COARDS, CF-1.10, ACDD-1.3</att>
        <att name="infoUrl">???</att>
        <att name="institution">USF IMaRS</att>
        <att name="keywords">water quality, Florida's Coral Reef, chlorphyll-a</att>
        <att name="license">[standard]</att>
        <att name="sourceUrl">(local files)</att>
        <att name="standard_name_vocabulary">CF Standard Name Table v70</att>
        <att name="subsetVariables">Parameter, Units</att>
        <att name="summary">Chlorophyll-a data from unified FCR database</att>
        <att name="title">Unified FCR Chla</att>
    </addAttributes>
    <dataVariable>
        <sourceName>Source</sourceName>
        <destinationName>Source</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">Source</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>Site</sourceName>
        <destinationName>Site</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">Site</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>Month</sourceName>
        <destinationName>Month</destinationName>
        <dataType>byte</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="_FillValue" type="byte">127</att>
            <att name="ioos_category">Time</att>
            <att name="long_name">Month</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>Year</sourceName>
        <destinationName>Year</destinationName>
        <dataType>short</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="_FillValue" type="short">32767</att>
            <att name="ioos_category">Time</att>
            <att name="long_name">Year</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>Parameter</sourceName>
        <destinationName>Parameter</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">Parameter</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>Value</sourceName>
        <destinationName>Value</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="_FillValue" type="double">NaN</att>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">Value</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>Units</sourceName>
        <destinationName>Units</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">Units</att>
        </addAttributes>
    </dataVariable>
</dataset>

